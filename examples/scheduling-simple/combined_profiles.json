        "args": {
            "flops": 1000
        }
    },
    "hard": {
        "type": "cpu-burn-homogenous",
        "args": {
            "flops": 1000000
        }
    },
    "test_parallel": {
        "type": "parallel",
        "args": {
            "profiles": [
                "simple",
                "hard",
                "simple",
