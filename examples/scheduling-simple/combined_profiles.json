    "advanced_parallel": {
        "type": "parallel",
        "args": {
            "profiles": [
                {
                    "type": "sequence",
                    "args": {
                        "repeat": 2,
                        "profiles": [
                            "simple",
                            "hard"
                        ]
                    }
                },
                {
                    "type": "sequence",
                    "args": {
                        "repeat": 4,
                        "profiles": [
                            "hard",
                            "hard",
                            "simple"
                        ]
                    }
                }
            ]
        }
